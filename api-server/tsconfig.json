{
  "compilerOptions": {
    "target": "ES2020",
    "module": "Preserve",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "baseUrl": ".",
    "outDir": "dist",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
